package fr.guillaume.sorter.sortthreads;

import java.util.ArrayList;

public abstract class AbstractSortThread extends Thread {
	protected ArrayList<Comparable<Object>> tab;
	
	public AbstractSortThread(ArrayList<Comparable> tab) {
		this.tab = tab;
	}
	
	@Override
	public void run() {
		sort();
	}
	
	public abstract void sort();
}
