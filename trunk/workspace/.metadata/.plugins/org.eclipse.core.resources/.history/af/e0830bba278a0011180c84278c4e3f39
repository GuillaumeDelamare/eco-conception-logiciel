package fr.guillaume.sorter.sortthreads;

public abstract class AbstractSortThread extends Thread {
	Comparable<Object> tab[];
	
	public AbstractSortThread(Comparable<Object>[] tab) {
		this.tab = tab;
	}
	
	@Override
	public void run() {
		// TODO Auto-generated method stub
		super.run();
	}
	
	public abstract void sort();
}
